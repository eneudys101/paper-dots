{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "palette": {
      "python_fg": "",
      "root_fg": "#8C0075",
      "os_fg": "#005656",
      "path_fg": "#3A7000",
      "time_fg": "#4B4B7D",
      "text_fg": "#404040"
    },
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "python",
            "style": "powerline",
            "foreground": "p:python_fg",
            "fetch_virtual_env": "true",
            "display_deafult": "base",
            "template": " \uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
          },
          {
            "type": "root",
            "style": "plain",
            "foreground": "#8C0075",
            "background": "transparent",
            "template": " \uf0e7 "
          },
          {
            "type": "os",
            "style": "plain",
            "foreground": "#005656",
            "background": "transparent",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
          },
          {
            "foreground": "p:blue",
            "style": "plain",
            "template": "{{ .UserName }}@{{ .HostName }} ",
            "type": "session"
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "#3A7000",
            "background": "transparent",
            "template": " {{ .Path }} ",
            "properties": {
              "folder_icon": "..\ue5fe..",
              "home_icon": "~",
              "style": "agnoster_short"
            }
          },
          {
            "foreground": "p:lavender",
            "properties": {
              "branch_icon": "\ue725 ",
              "cherry_pick_icon": "\ue29b ",
              "commit_icon": "\uf417 ",
              "fetch_status": false,
              "fetch_upstream_icon": false,
              "merge_icon": "\ue727 ",
              "no_commits_icon": "\uf594 ",
              "rebase_icon": "\ue728 ",
              "revert_icon": "\uf0e2 ",
              "tag_icon": "\uf412 "
            },
            "template": "{{ .HEAD }} ",
            "style": "plain",
            "type": "git"
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "right",
        "segments": [
          {
            "type": "time",
            "style": "plain",
            "foreground": "#4B4B7D",
            "background": "transparent",
            "template": " {{ .CurrentDate | date .Format }} ",
            "properties": {
              "time_format": "2:04:05"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#404040",
            "background": "transparent",
            "template": " âžœ "
          }
        ]
      }
    ],
    "osc99": true
  }
  