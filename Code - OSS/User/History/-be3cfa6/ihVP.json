{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "python",
          "style": "powerline",
          "foreground": "#171C3B",
          "background": "#F9F3CC",
          "fetch_virtual_env": "true",
          "display_deafult": "base",
          "template": " \uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#8C0075",
          "background": "#F9F3CC",
          "template": " \uf0e7 "
        },
        {
          "type": "os",
          "style": "plain",
          "foreground": "#005656",
          "background": "#F9F3CC",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
        },
        {
          "foreground": "#171C3B",
          "background": "#E8E3C4",
          "style": "plain",
          "template": "{{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#3A7000",
          "background": "#F9F3CC",
          "template": " {{ .Path }} ",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          }
        },
        {
          "foreground": "#8C0075",
          "background": "#F9F3CC",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }} ",
          "style": "plain",
          "type": "git"
        }
      ],
      {
        "type": "prompt",
        "alignment": "right",
        "segments": [
          {
            "type": "time",
            "style": "plain",
            "foreground": "#4B4B7D",
            "background": "#F9F3CC",
            "template": " {{ .CurrentDate | date .Format }} ",
            "properties": {
              "time_format": "2:04:05"
            }
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "#404040",
            "background": "#F9F3CC",
            "template": " âžœ "
          }
        ]
      }
  ],
  "osc99": true
}
