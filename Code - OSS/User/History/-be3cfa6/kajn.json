{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "final_space": true,
    "palette": {
      "python_fg": "#FFA726",
      "root_fg": "#32CD32",
      "os_fg": "#1E90FF",
      "session_fg": "#FF6347",
      "path_fg": "#228B22",
      "git_fg": "#FFD700",
      "time_fg": "#708090",
      "prompt_fg": "#404040"
    },       
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
          {
            "type": "python",
            "style": "powerline",
            "foreground": "p:python_fg",
            "fetch_virtual_env": "true",
            "display_deafult": "base",
            "template": " \uE235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} "
          },
          {
            "type": "root",
            "style": "plain",
            "foreground": "p:root_fg",
            "background": "transparent",
            "template": " \uf0e7 "
          },
          {
            "type": "os",
            "style": "plain",
            "foreground": "p:os_fg",
            "background": "transparent",
            "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} "
          },
          {
            "foreground": "p:session_fg",
            "style": "plain",
            "template": "{{ .UserName }}@{{ .HostName }} ",
            "type": "session"
          },
          {
            "type": "path",
            "style": "plain",
            "foreground": "p:path_fg",
            "background": "transparent",
            "template": " {{ .Path }} ",
            "properties": {
              "folder_icon": "..\ue5fe..",
              "home_icon": "~",
              "style": "agnoster_short"
            }
          },
          {
            "foreground": "p:git_fg",
            "properties": {
              "branch_icon": "\ue725 ",
              "cherry_pick_icon": "\ue29b ",
              "commit_icon": "\uf417 ",
              "fetch_status": false,
              "fetch_upstream_icon": false,
              "merge_icon": "\ue727 ",
              "no_commits_icon": "\uf594 ",
              "rebase_icon": "\ue728 ",
              "revert_icon": "\uf0e2 ",
              "tag_icon": "\uf412 "
            },
            "template": "{{ .HEAD }} ",
            "style": "plain",
            "type": "git"
          }
        ]
      },
      {
        "type": "prompt",
        "alignment": "left",
        "newline": true,
        "segments": [
          {
            "type": "text",
            "style": "plain",
            "foreground": "p:prompt_fg",
            "background": "transparent",
            "template": " âžœ "
          }
        ]
      }
    ],
    "osc99": true
  }
  